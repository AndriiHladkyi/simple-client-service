<h1>Delivery Types</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @delivery_types.each do |delivery_type| %>
      <tr>
        <td><%= delivery_type.name %></td>
        <td><%= link_to 'Show', delivery_type, class: 'btn btn-dark' %></td>
        <td><%= link_to 'Edit', edit_delivery_type_path(delivery_type), class: 'btn btn-dark' %></td>
        <td><%= link_to 'Destroy', delivery_type, method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Delivery Type', new_delivery_type_path, class: 'btn btn-dark' %>
